<?xml version="1.0" encoding="UTF-8" ?>
<Patch>
    <Operation Class="PatchOperationAdd">
        <xpath>Defs/ThingDef[defName="BabyFood"]</xpath>
        <value>
            <projectileWhenLoaded>MSSFP_BabyProjectile</projectileWhenLoaded>
        </value>
    </Operation>
    <Operation Class="PatchOperationAdd">
        <xpath>Defs/ThingDef[defName="BabyFood"]</xpath>
        <value><tickerType>Normal</tickerType></value>
    </Operation>
    <Operation Class="PatchOperationAdd">
        <xpath>Defs/ThingDef[defName="BabyFood"]/comps</xpath>
        <value>
            <li Class="CompProperties_Explosive">
                <explosiveRadius>2.9</explosiveRadius>
                <explosiveDamageType>MSSFP_BabyBomb</explosiveDamageType>
                <explosiveExpandPerStackcount>0.4</explosiveExpandPerStackcount>
                <startWickHitPointsPercent>0.7</startWickHitPointsPercent>
                <wickTicks>30~60</wickTicks>
            </li>
        </value>
    </Operation>

</Patch>
