<?xml version="1.0" encoding="UTF-8" ?>
<Defs>
    <GeneDef ParentName="AG_BeginningSummonBase">
        <defName>MSSFP_FroggeAffinity</defName>
        <label>frogge affinity</label>
        <description>Carriers of this gene have a mystical psychic bond with frogge</description>
        <iconPath>UI/Icons/Genes/MSSFP_FroggeAffinity</iconPath>
        <displayOrderInCategory>80</displayOrderInCategory>
        <customEffectDescriptions>
            <li>Begins play with a tamed frogge.</li>
            <li>This gene can only be chosen (and only has effects) at character creation. It won't appear during gameplay.</li>
        </customEffectDescriptions>
        <selectionWeight>0</selectionWeight>
        <biostatMet>-3</biostatMet>
        <symbolPack>
            <prefixSymbols>
                <li>
                    <symbol>frogge</symbol>
                </li>
            </prefixSymbols>
        </symbolPack>
        <modExtensions>
            <li Class="VanillaGenesExpanded.GeneExtension">
                <backgroundPathEndogenes>UI/Icons/Genes/AG_Endogenes</backgroundPathEndogenes>
                <backgroundPathXenogenes>UI/Icons/Genes/AG_Xenogenes</backgroundPathXenogenes>
                <thingSetMaker>MSSFP_Frogge</thingSetMaker>
            </li>
        </modExtensions>
    </GeneDef>

    <ThingSetMakerDef>
        <defName>MSSFP_Frogge</defName>
        <root Class="MSSFP.Genes.ThingSetMaker_Frogge"/>
    </ThingSetMakerDef>

    <GeneDef ParentName="AG_BeginningSummonBase">
        <defName>MSSFP_JroggeAffinity</defName>
        <label>frogge affinity</label>
        <description>Carriers of this gene have a very strange interest in the politics of Old Earth, polar bears, and mushrooms. You'd think the overlap of those things would be zero, but here we are.</description>
        <iconPath>UI/Icons/Genes/MSSFP_JroggeAffinity</iconPath>
        <displayOrderInCategory>80</displayOrderInCategory>
        <customEffectDescriptions>
            <li>Begins play with a tamed jrogge.</li>
            <li>This gene can only be chosen (and only has effects) at character creation. It won't appear during gameplay.</li>
        </customEffectDescriptions>
        <selectionWeight>0</selectionWeight>
        <biostatMet>-3</biostatMet>
        <symbolPack>
            <prefixSymbols>
                <li>
                    <symbol>frogge</symbol>
                </li>
            </prefixSymbols>
        </symbolPack>
        <modExtensions>
            <li Class="VanillaGenesExpanded.GeneExtension">
                <backgroundPathEndogenes>UI/Icons/Genes/AG_Endogenes</backgroundPathEndogenes>
                <backgroundPathXenogenes>UI/Icons/Genes/AG_Xenogenes</backgroundPathXenogenes>
                <thingSetMaker>MSSFP_Jrogge</thingSetMaker>
            </li>
        </modExtensions>
    </GeneDef>

    <ThingSetMakerDef>
        <defName>MSSFP_Jrogge</defName>
        <root Class="MSSFP.Genes.ThingSetMaker_Jrogge"/>
    </ThingSetMakerDef>
</Defs>
